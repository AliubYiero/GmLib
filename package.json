{
	"name": "@yiero/gmlib",
	"author": "Yiero",
	"version": "0.1.14",
	"description": "GM Lib for Tampermonkey",
	"license": "MIT",
	"keywords": [
		"tampermonkey",
		"lib",
		"scriptcat",
		"utils"
	],
	"type": "module",
	"scripts": {
		"build": "vite build && npm run build:types",
		"build:types": "tsc && dts-bundle-generator -o dist/index.d.ts src/index.ts && rmdir /s /q \"dist/types\"",
		"publish": "nrm use npm & npm run build & npm publish",
		"switch-taobao-npm": "nrm use taobao"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"dts-bundle-generator": "^9.5.1",
		"typescript": "^5.2.2",
		"vite": "^5.1.4",
		"vite-plugin-banner": "^0.8.0"
	},
	"module": "./dist/index.mjs",
	"main": "dist/index.mjs",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"types": "./dist/types/index.d.ts"
		}
	},
	"dependencies": {
		"@types/tampermonkey": "^5.0.4"
	},
	"private": false,
	"files": [
		"dist",
		"LICENSE"
	],
	"repository": {
		"url": "git+https://github.com/AliubYiero/GmLib.git"
	}
}
